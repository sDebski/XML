<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd=
"http://www.w3.org/2001/XMLSchema">
	<xsd:element name="cena" type="cena_type">
	</xsd:element>

	<xsd:simpleType name="cena_type">
               <xsd:restriction base="xsd:string">
                        <xsd:pattern value="([1-9][0-9]*\.[0-9]{2})|([0]\.[0-9]{2})" />
                </xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
